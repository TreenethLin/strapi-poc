{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "release": {
      "type": "datetime"
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "Markdown",
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "cta_url": {
      "type": "string"
    },
    "cta_btn": {
      "type": "enumeration",
      "enum": [
        "Subscribe",
        "Learn More",
        "Get Started",
        "Contact Us",
        "Download Now",
        "Buy Now",
        "Request Demo",
        "Sign Up",
        "Join Now",
        "Try for Free"
      ]
    }
  }
}
